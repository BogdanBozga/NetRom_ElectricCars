@{
    ViewData["Title"] = "Percentage";
}








@model Statistics
<canvas id="myChart" width="400" height="400"></canvas>
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script>
const ctx = document.getElementById('myChart').getContext('2d');
const myChart = new Chart(ctx, {
    type: 'bar',
    data: {
        labels: ['has.to.be Charging Station','be.energised Charging Station','Renovatio e-charge Charging Station','test'],
        datasets: [{
            label: '# of Votes',
            data: [1.5,0,0,0],
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)'

            ],
            borderColor: [
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    },
    options: {
        scales: {
            y: {
                beginAtZero: true,
                suggestedMax : 100
            }
        }
    }
});
</script>


